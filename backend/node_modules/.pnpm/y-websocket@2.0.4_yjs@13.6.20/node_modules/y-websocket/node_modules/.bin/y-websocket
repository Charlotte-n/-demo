#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/merikle/Desktop/ui/common/backend/node_modules/.pnpm/y-websocket@2.0.4_yjs@13.6.20/node_modules/y-websocket/bin/node_modules:/Users/merikle/Desktop/ui/common/backend/node_modules/.pnpm/y-websocket@2.0.4_yjs@13.6.20/node_modules/y-websocket/node_modules:/Users/merikle/Desktop/ui/common/backend/node_modules/.pnpm/y-websocket@2.0.4_yjs@13.6.20/node_modules:/Users/merikle/Desktop/ui/common/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/merikle/Desktop/ui/common/backend/node_modules/.pnpm/y-websocket@2.0.4_yjs@13.6.20/node_modules/y-websocket/bin/node_modules:/Users/merikle/Desktop/ui/common/backend/node_modules/.pnpm/y-websocket@2.0.4_yjs@13.6.20/node_modules/y-websocket/node_modules:/Users/merikle/Desktop/ui/common/backend/node_modules/.pnpm/y-websocket@2.0.4_yjs@13.6.20/node_modules:/Users/merikle/Desktop/ui/common/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/server.cjs" "$@"
else
  exec node  "$basedir/../../bin/server.cjs" "$@"
fi
